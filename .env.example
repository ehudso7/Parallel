# ===========================================
# PARALLEL - Environment Variables
# ===========================================

# App
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Admin
ADMIN_SECRET_KEY=your_admin_secret_key

# AI Providers (alphabetically ordered)
ANTHROPIC_API_KEY=your_anthropic_api_key
OPENAI_API_KEY=your_openai_api_key

# AI Creation APIs (alphabetically ordered)
ELEVENLABS_API_KEY=your_elevenlabs_api_key
FAL_API_KEY=your_fal_api_key
LUMA_API_KEY=your_luma_api_key
REPLICATE_API_TOKEN=your_replicate_token
RUNWAY_API_KEY=your_runway_api_key
SUNO_API_KEY=your_suno_api_key

# Analytics
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com
NEXT_PUBLIC_POSTHOG_KEY=your_posthog_public_key
POSTHOG_API_KEY=your_posthog_api_key

# Caching
UPSTASH_REDIS_REST_TOKEN=your_upstash_token
UPSTASH_REDIS_REST_URL=your_upstash_url
REDIS_URL=your_redis_url

# Payments - Mobile (RevenueCat)
REVENUECAT_API_KEY=your_revenuecat_api_key
REVENUECAT_APPLE_API_KEY=your_revenuecat_apple_key
REVENUECAT_GOOGLE_API_KEY=your_revenuecat_google_key

# Payments - Web (Stripe)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Stripe Price IDs (create these in Stripe Dashboard)
# IMPORTANT: These must be valid Stripe price IDs before production deployment
# Subscription tiers: Free=$0, Basic=$19.99, Pro=$29.99, Unlimited=$49.99, Studio=$99.99
STRIPE_PRICE_BASIC=price_basic_monthly
STRIPE_PRICE_PRO=price_pro_monthly
STRIPE_PRICE_STUDIO=price_studio_monthly
STRIPE_PRICE_UNLIMITED=price_unlimited_monthly

# Push Notifications
EXPO_ACCESS_TOKEN=your_expo_access_token
ONESIGNAL_API_KEY=your_onesignal_api_key
ONESIGNAL_APP_ID=your_onesignal_app_id

# Supabase
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
